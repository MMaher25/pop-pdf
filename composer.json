{
    "name": "popphp/pop-pdf",
    "description": "PHP PDF library for generating and importing PDF documents.",
    "keywords": [
        "pdf",
        "create pdf",
        "import pdf",
        "pdf generation",
        "php pdf library",
        "php pdf processing"
    ],
    "homepage": "http://github.com/popphp/pop-pdf",
    "license": "New BSD",
    "authors": [
        {
            "name": "Nick Sagona",
            "email": "dev@nolainteractive.com",
            "homepage": "http://www.nolainteractive.com/"
        }
    ],
    "require": {
        "php": ">=5.4.0"
    },
    "suggest": {
        "ext-gd": "For the image parser"
    },
    "autoload": {
        "psr-4": {
            "Pop\\Pdf\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        }
    }
}

